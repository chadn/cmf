{
    "name": "calendar-map-filter",
    "version": "0.4.9",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:https": "node src/scripts/server.js",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "format:diff": "npx prettier $FN | git --no-pager diff --no-index $FN -",
        "test": "jest --coverage && node src/scripts/show-total-loc.mjs && date && echo",
        "test:e2e": "playwright test",
        "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts",
        "test:e2e:mobile": "playwright test --project=mobile-iphone16",
        "test:e2e:full": "playwright test --project=desktop-chrome --project=mobile-iphone16",
        "test:e2e:console": "TEST_URL=${TEST_URL:-/} playwright test tests/e2e/console-logs.spec.ts --headed",
        "test:e2e:pageload": "playwright test tests/e2e/page-load.spec.ts --headed",
        "test:e2e:interactive": "playwright test tests/e2e/interactive.spec.ts --headed",
        "test:report": "playwright show-report",
        "cache": "node --experimental-transform-types src/scripts/upstash-redis.ts"
    },
    "dependencies": {
        "@math.gl/web-mercator": "^4.1.0",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@shadcn/ui": "^0.0.4",
        "@upstash/redis": "^1.35.5",
        "@vercel/functions": "^3.1.3",
        "@vercel/kv": "^3.0.0",
        "axios": "^1.12.2",
        "axios-curlirize": "^2.0.0",
        "cheerio": "^1.1.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^16.6.1",
        "ical.js": "^2.2.1",
        "lucide-react": "^0.545.0",
        "luxon": "^3.7.2",
        "mapbox-gl": "^3.15.0",
        "maplibre-gl": "^5.8.0",
        "next": "^15.5.4",
        "nuqs": "^2.7.1",
        "react": "^19.2.0",
        "react-day-picker": "^9.11.1",
        "react-dom": "^19.2.0",
        "react-map-gl": "^8.1.0",
        "react-resizable-panels": "^3.0.6",
        "react-toastify": "^11.0.5",
        "swr": "^2.3.6",
        "tailwind-merge": "^3.3.1",
        "tailwindcss-animate": "^1.0.7",
        "tz-lookup": "^6.1.25"
    },
    "devDependencies": {
        "@playwright/test": "^1.56.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@types/axios-curlirize": "^1.3.5",
        "@types/cheerio": "^0.22.35",
        "@types/jest": "^29.5.14",
        "@types/luxon": "^3.7.1",
        "@types/node": "^20.19.19",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@types/tz-lookup": "^6.1.2",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.57.1",
        "eslint-config-next": "^15.5.4",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "tailwindcss": "^3.4.18",
        "typescript": "^5.9.3"
    }
}
